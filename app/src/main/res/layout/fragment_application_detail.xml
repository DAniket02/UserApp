<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".fragment.ApplicationDetailFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="@dimen/marginplus2">

            <TextView
                android:id="@+id/textview_appDetail_appId"
                android:layout_margin="@dimen/marginplus1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_appid"
                android:textSize="@dimen/textsizemlarge"
                android:textColor="@color/black_font1"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="@dimen/marginplus1"
                android:layout_marginRight="@dimen/marginplus1"
                android:layout_marginTop="@dimen/marginplus2">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="1dp"
                            android:layout_height="45dp"
                            android:background="@color/red_font1"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/marginplus2">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black_font4"
                                android:text="@string/text_application_date"/>

                            <TextView
                                android:id="@+id/textview_appDetail_appDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black_font3"
                                android:textSize="@dimen/textsizesmall"
                                android:textStyle="bold"
                                android:text="@string/text_date2"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="1dp"
                            android:layout_height="45dp"
                            android:background="@color/red_font1"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/marginplus2">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black_font4"
                                android:text="@string/text_expected_completion"/>

                            <TextView
                                android:id="@+id/textview_appDetail_exp_completion_appDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black_font3"
                                android:textSize="@dimen/textsizesmall"
                                android:textStyle="bold"
                                android:text="@string/text_date2"/>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="@dimen/marginplus1"
                android:layout_marginRight="@dimen/marginplus1"
                android:layout_marginTop="@dimen/marginplus3">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="1dp"
                            android:layout_height="45dp"
                            android:background="@color/red_font1"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/marginplus2">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black_font4"
                                android:text="@string/text_service_name"/>

                            <TextView
                                android:id="@+id/textview_appDetail_serviceName"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black_font3"
                                android:textSize="@dimen/textsizesmall"
                                android:textStyle="bold"
                                android:text="@string/text_trimingofbranches"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="1dp"
                            android:layout_height="45dp"
                            android:background="@color/red_font1"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/marginplus2">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black_font4"
                                android:text="@string/text_status"/>

                            <Button
                                android:id="@+id/button_appDetail_status"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:minHeight="20dp"
                                android:layout_marginTop="@dimen/marginplus1"
                                android:textSize="@dimen/textsizeminismall"
                                android:text="@string/status_completed"
                                android:textColor="@color/white_color"
                                android:background="@color/application_completed_green"/>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAllCaps="true"
                    android:textSize="@dimen/textsizemlarge"
                    android:text="@string/text_application_details"
                    android:layout_marginRight="@dimen/marginplus1"
                    android:layout_marginTop="@dimen/marginplus6"
                    android:layout_marginLeft="@dimen/marginplus1"
                    android:textColor="@color/black_font1"/>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:cardCornerRadius="7dp"
                android:layout_marginTop="@dimen/marginplus2">

                <LinearLayout
                    android:background="@drawable/rounded_corner_rect"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:background="@drawable/up_round_rect">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/toolbar_color"
                            android:textStyle="bold"
                            android:text="@string/text_personalinfo"
                            android:layout_margin="@dimen/marginplus2"/>

                    </LinearLayout>

                    <!--<TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/black_font6"/>-->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/marginplus3">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/text_applicant_name"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_applicantName"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="@string/text_applicant_name"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/text_appicant_name_in_marathi"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_applicantNameMarathi"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="@string/text_applicant_name"/>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/marginplus3">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/textmobile2"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_applicantMobile"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="@string/text_dummy_mobile"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/textemail2"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_applicantEmail"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="@string/text_dummy_email"/>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/marginplus3"
                        android:layout_marginBottom="@dimen/marginplus2">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/textaddress2"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_applicantAddress"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="@string/textaddress2"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/text_applicant_address_in_marathi"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_applicantAddressMarathi"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="@string/text_applicant_name"/>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:cardCornerRadius="7dp"
                android:layout_marginTop="@dimen/marginplus2"
                android:layout_marginBottom="@dimen/marginplus2">

                <LinearLayout
                    android:background="@drawable/rounded_corner_rect"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:background="@drawable/up_round_rect">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/toolbar_color"
                            android:textStyle="bold"
                            android:text="@string/text_treecuttinginfo"
                            android:layout_margin="@dimen/marginplus2"/>

                    </LinearLayout>

                    <!--<TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/black_font6"/>-->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/marginplus3">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/text_tree_type"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_treeType"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="@string/text_prajati"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/marginplus2">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font4"
                                    android:text="@string/text_total"/>

                                <TextView
                                    android:id="@+id/textview_appDetail_treeTotal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black_font3"
                                    android:textSize="@dimen/textsizesmall"
                                    android:textStyle="bold"
                                    android:text="00"/>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/marginplus3">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/marginplus2">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font4"
                                        android:text="@string/text_reason"/>

                                    <TextView
                                        android:id="@+id/textview_appDetail_reason"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font3"
                                        android:textSize="@dimen/textsizesmall"
                                        android:textStyle="bold"
                                        android:text="@string/text_dummy_text"/>

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/marginplus2">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font4"
                                        android:text="@string/text_ward"/>

                                    <TextView
                                        android:id="@+id/textview_appDetail_ward"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font3"
                                        android:textSize="@dimen/textsizesmall"
                                        android:textStyle="bold"
                                        android:text="15"/>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/marginplus3"
                            android:layout_marginBottom="@dimen/marginplus2">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/marginplus2">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font4"
                                        android:text="@string/text_department"/>

                                    <TextView
                                        android:id="@+id/textview_appDetail_departmentName"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font3"
                                        android:textSize="@dimen/textsizesmall"
                                        android:textStyle="bold"
                                        android:text="@string/text_applicant_name"/>

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:visibility="invisible"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/marginplus2">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font4"
                                        android:text="@string/textaddress2"/>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_font3"
                                        android:textSize="@dimen/textsizesmall"
                                        android:textStyle="bold"
                                        android:text="@string/text_applicant_name"/>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="7dp"
                android:layout_marginBottom="@dimen/marginplus2"
                android:layout_marginTop="@dimen/marginplus2">

                <LinearLayout
                    android:background="@drawable/rounded_corner_rect"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/up_round_rect">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/toolbar_color"
                        android:textStyle="bold"
                        android:text="@string/text_tree_images"
                        android:layout_margin="@dimen/marginplus2"/>

                </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_margin="@dimen/marginplus1">

                        <ImageView
                            android:id="@+id/imageview_appDetail_pic1"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:src="@drawable/nmmc_tree"
                            android:scaleType="fitCenter"
                            android:layout_margin="1dp"
                            android:layout_height="120dp"/>

                        <ImageView
                            android:id="@+id/imageview_appDetail_pic2"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:src="@drawable/nmmc_tree"
                            android:scaleType="fitCenter"
                            android:layout_margin="1dp"
                            android:layout_height="120dp"/>

                        <ImageView
                            android:id="@+id/imageview_appDetail_pic3"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:src="@drawable/nmmc_tree"
                            android:scaleType="fitCenter"
                            android:layout_margin="1dp"
                            android:layout_height="120dp"/>

                    </LinearLayout>

                </LinearLayout>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:cardCornerRadius="7dp"
                android:layout_marginBottom="@dimen/marginplus4"
                android:layout_marginTop="@dimen/marginplus2">

                <LinearLayout
                    android:background="@drawable/rounded_corner_rect"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
